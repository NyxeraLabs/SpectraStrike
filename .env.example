# SpectraStrike exposed host ports (custom defaults)
HOST_PROXY_HTTP_PORT=18080
HOST_PROXY_TLS_PORT=18443
HOST_DB_PORT=15432
HOST_RABBITMQ_MGMT_PORT=15672

# Remote integration endpoints
MSF_RPC_HOST=metasploit.remote.operator
MSF_RPC_PORT=55553
MSF_RPC_SSL=true
# Optional cert pin (sha256 hex)
MSF_RPC_TLS_PINNED_CERT_SHA256=

MSF_MANUAL_BASE_URL=https://metasploit.remote.operator:5443
MSF_MANUAL_USERNAME=
MSF_MANUAL_PASSWORD=

# RabbitMQ remote broker (if not using in-stack broker)
RABBITMQ_HOST=rabbitmq
RABBITMQ_PORT=5671
RABBITMQ_USER=spectra
RABBITMQ_PASSWORD=
RABBITMQ_SSL=true
RABBITMQ_SSL_CA_FILE=/app/docker/pki/ca.crt
RABBITMQ_SSL_CERT_FILE=/app/docker/pki/app/client.crt
RABBITMQ_SSL_KEY_FILE=/app/docker/pki/app/client.key

# Optional VectorVue pin (sha256 hex)
VECTORVUE_TLS_PINNED_CERT_SHA256=

# Optional public egress CIDR allowlist for firewall-egress-apply
# Example: EGRESS_ALLOW_CIDRS=\"1.2.3.4/32 5.6.7.0/24\"
EGRESS_ALLOW_CIDRS=
